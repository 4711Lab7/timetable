<?xml version='1.0' encoding='UTF-8'?>

<!--must be in this order-->
<!ELEMENT timetable (timeslots, daysofweek, courses)>

<!ELEMENT timeslots (time)*>

<!ELEMENT time (class)*>
<!ATTLIST time
    start CDATA #REQUIRED
>

<!--- Put your DTDDoc comment here. -->
<!ELEMENT class EMPTY>
<!ATTLIST class
    location CDATA #REQUIRED
    day (monday | tuesday | wednesday | thursday | friday) #REQUIRED
    course (blaw3600 | comp4711 | comp4560 | comp4735 | comp4941 | comp4945) #REQUIRED
    instructor CDATA #REQUIRED
    end CDATA #REQUIRED
    start CDATA #REQUIRED
>
<!--must be in this order-->
<!ELEMENT daysofweek (monday, tuesday, wednesday, thursday, friday)>

<!ELEMENT monday (class)*>

<!ELEMENT tuesday (class)*>

<!ELEMENT wednesday (class)*>

<!ELEMENT thursday (class)*>

<!ELEMENT friday (class)*>

<!--courses must be listed out in this order-->
<!ELEMENT courses (blaw3600, comp4711, comp4560, comp4735, comp4941, comp4945)>

<!ELEMENT blaw3600 (class)*>

<!ELEMENT comp4711 (class)*>

<!ELEMENT comp4560 (class)*>

<!ELEMENT comp4735 (class)*>

<!ELEMENT comp4941 (class)*>

<!ELEMENT comp4945 (class)*>
